    "version": "1.0.0",
    "description": "Typescript SDKs for the Zebec Protocol",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/Zebec-protocol/zebec-sdk-v2.git"
    },
    "keywords": [
        "zebec-protocol",
        "money-streaming",
        "token streaming",
        "zebec"
    ],
    "license": "ISC",
    "bugs": {
